<view class="card-container">
  <view class="top" style="background:url('{{imageUrl+itemData.categoryCover}}')" bindtap="toGoodDetail">
    <!-- <image src="../../images/Food1.jpg" mode="widthFix"/> -->
    <view class="content">
      <text class="name">{{itemData.categoryName}}</text>
      <text class="desc">{{itemData.categoryIntroduction}}</text>
      <view class="rate">
        <van-icon name="star" color="rgb(242,203,81)" size="25px" />
        <text>4.9</text>
      </view>
      <text class="time">10min</text>
    </view>
  </view>
</view>